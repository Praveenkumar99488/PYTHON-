    "def count_even_digits(n):\n",
    "    if n < 1:\n",
    "        raise ValueError\n",
    "    \n",
    "    count = 0\n",
    "    while n > 0:\n",
    "        digit = n % 10  \n",
    "        if digit % 2 == 0: \n",
    "            count += 1  \n",
    "        n //= 10  \n",
    "    \n",
    "    return count\n",
    "\n",
    "number = 246135\n",
    "print(count_even_digits(number))\n"
   ]
  }
