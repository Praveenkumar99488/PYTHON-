    "def list_info(numbers):\n",
    "    length = 0 \n",
    "    total_sum = 0 \n",
    "\n",
    "    for number in numbers:  \n",
    "        length += 1  \n",
    "        total_sum += number  \n",
    "    \n",
    "    return length, total_sum\n",
    "\n",
    "\n",
    "number_list = [1, 2, 3, 4, 5]\n",
    "total = list_info(number_list)\n",
    "print(list_info(number_list))\n",
    "\n"
