    "def count_even_numbers(numbers):\n",
    "    even_count = 0\n",
    "    for num in numbers:\n",
    "        if num % 2 == 0:\n",
    "            even_count += 1\n",
    "    return even_count\n",
    "\n",
    "\n",
    "numbers = [1, 2, 3, 4, 5, 6]\n",
    "even_count = count_even_numbers(numbers)\n",
    "print(even_count) \n"
   ]
